{
    "name": "uccello/country",
    "description": "List of all countries for Uccello.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan SARDO",
            "email": "jonathan@uccellolabs.com"
        }
    ],
    "require": {
        "uccello/uccello": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Uccello\\Country\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uccello\\Country\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uccello\\Country\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
